<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Login Page</title>
  </head>
  <body>
    <h1>Login Page</h1>
    <%= form_tag '/welcome' %>
      <%= label_tag :name %>
      <%= text_field_tag :name %>
      <%= label_tag :password %>
      <%= text_field_tag :password %>
      <%= submit_tag %>
    <% end %>
  </body>
</html>
